<?xml version="1.0" encoding="UTF-8"?>

<!-- User Interface for Restaurant Manager program-->

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="255.0" maxWidth="465.0" minHeight="255.0" minWidth="465.0" prefHeight="255.0" prefWidth="465.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.RMController">
   <children>
      <Button fx:id="addPersonButton" layoutX="14.0" layoutY="112.0" mnemonicParsing="false" onAction="#addPersonClick" prefHeight="31.0" prefWidth="119.0" text="Add Person" textAlignment="CENTER" />
      <Button fx:id="editPersonButton" layoutX="173.0" layoutY="113.0" mnemonicParsing="false" onAction="#editPersonClick" prefHeight="31.0" prefWidth="119.0" text="Edit Person" textAlignment="CENTER" />
      <Button fx:id="delPersonButton" layoutX="332.0" layoutY="113.0" mnemonicParsing="false" onAction="#delPersonClick" prefHeight="31.0" prefWidth="119.0" text="Delete Person" textAlignment="CENTER" />
      <Button fx:id="viewPersonButton" layoutX="173.0" layoutY="197.0" mnemonicParsing="false" onAction="#viewPersonClick" prefHeight="31.0" prefWidth="119.0" text="View List" textAlignment="CENTER" />
      <Label layoutX="108.0" layoutY="28.0" text="Restaurant Managing" textAlignment="CENTER">
         <font>
            <Font name="Arial" size="26.0" />
         </font>
      </Label>
   </children>
</Pane>
